{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "package.json"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "prisma:generate": {
      "outputs": ["node_modules/.prisma/**"]
    }
  }
}
